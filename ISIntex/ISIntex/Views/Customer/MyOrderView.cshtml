@model IEnumerable<ISIntex.Models.RejectedEstimate>

@{
    ViewBag.Title = "MyOrderView";
}

<h2>MyOrderView</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CustomerID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comments)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LTNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstimatedPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AssayID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Element1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Element2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Element1Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Element2Quantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LTNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstimatedPrice)
        </td>
        <td>
