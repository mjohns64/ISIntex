        <td>
            @Html.DisplayFor(modelItem => item.Element2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Element1Quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Element2Quantity)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EstimateID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EstimateID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EstimateID })
        </td>
    </tr>
}

</table>
