@model ISIntex.Models.WorkOrder

@{
    ViewBag.Title = "Add Test Result";
}

<h2 style="padding-left:20px">Add Test Result</h2>

    

    <div class="form-horizontal" style="padding-left:20px">
        <h4>LT Number: @Html.DisplayFor(model => model.LTNumber)</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <div class="col-md-10">
                @using (Html.BeginForm("AddTestResult", "Technician", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                    <div>
                        <br />
                        <input type="file" name="fileUpload" id="fileUpload" /><br />

                    </div>
                    @Html.HiddenFor(x => Model.LTNumber)
                    <div class="form-group" style="margin-top:50px">
                        <input type="submit" class="btn btn-default" value="Upload">
                    </div>
                }
            </div>
        </div>
    </div>

<div>
    @Html.ActionLink("Back to List", "WorkOrders")
</div>

<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
